{
  "name": "@trifle/trifle-hub",
  "version": "1.0.80",
  "description": "Vue component library for Trifle Hub",
  "keywords": [
    "vue",
    "components",
    "trifle",
    "hub"
  ],
  "license": "ISC",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "author": "Trifle UG",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/*": "./dist/*"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "style": "dist/trifle-hub.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "cd dev && vite",
    "watch": "vite build --watch",
    "prepublishOnly": "yarn build",
    "patch": "npm version patch && npm publish && git push"
  },
  "dependencies": {
    "@farcaster/auth-client": "^0.6.0",
    "@farcaster/frame-sdk": "^0.0.57",
    "@farcaster/frame-wagmi-connector": "^0.0.42",
    "@reown/appkit": "^1.7.8",
    "@reown/appkit-adapter-wagmi": "^1.7.8",
    "@reown/appkit-siwe": "^1.7.8",
    "@tanstack/vue-query": "^5.75.1",
    "@vueuse/core": "^10.0.0",
    "@wagmi/core": "^2.17.2",
    "@wagmi/vue": "^0.1.21",
    "pinia": "^3.0.2",
    "tailwindcss": "^3.4.15",
    "three": "^0.176.0",
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/eslint-parser": "^7.27.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.26.0",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.1.0",
    "postcss": "^8.4.49",
    "vite": "^6.0.1",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^10.1.3",
    "vue-router": "^4.5.0"
  },
  "peerDependencies": {
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  }
}
